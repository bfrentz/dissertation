doi = {10.1103/PhysRevC.81.055807},
file = {:Users/Bryce/Research/Mendeley/Marta et al/Marta et al.. 2010.pdf:pdf},
issn = {05562813},
journal = {Phys. Rev. C - Nucl. Phys.},
